[user]
	name = Furkhat Kasymov Genii Uulu
	email = vailodf@gmail.com
[alias]
	st = status -sb
	ci = commit
	co = checkout
	last = log -1
	br = branch
[color]
	ui = auto
[color "branch"]
	cerrunt = yellow reverse
	local = yellow
	remote = green
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[core]
	whitespace=fix,-indent-with-non-tab,trailing-space,cr-at-eol
	editor = vim
	quotepath = false
[push]
	default = current
[url "git@github.com:"]
	insteadOf = https://github.com/
[pull]
	rebase = false

